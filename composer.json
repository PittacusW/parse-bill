{
  "name": "pittacusw/parse-bill",
  "description": "Wrapper to parse XML Bills and generate a PDF file",
  "keywords": [
    "laravel",
    "touchef"
  ],
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Touchef",
      "email": "contacto@touchef.cl"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "pittacusw/core": "dev-main",
    "barryvdh/laravel-dompdf": "^2.0",
    "freshwork/chilean-bundle": "^2.1",
    "laravel/framework": "^9.23|^12.0",
    "neitanod/forceutf8": "^2.0",
    "spatie/laravel-package-tools": "^1.12",
    "milon/barcode": "^9.0",
    "spatie/array-to-xml": "^3.1"
  },
  "autoload": {
    "psr-0": {
      "Pittacusw\\ParseBill": "src/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pittacusw\\ParseBill\\ParseBillServiceProvider"
      ]
    }
  }
}
